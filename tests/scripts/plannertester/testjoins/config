SELECT * FROM R1 JOIN R2 ON R1.A = R2.A;
SELECT * FROM R1 JOIN R2 USING(A);
SELECT * FROM P1 JOIN R2 ON P1.A = R2.A;
SELECT * FROM R1 JOIN R2 ON R1.A = R2.A WHERE R1.C > R2.C;
SELECT * FROM R1 JOIN R2 ON R1.A = R2.A WHERE R1.C > R2.C;
SELECT * FROM R1 INNER JOIN R2 ON R1.A = R2.A WHERE R1.C > R2.C;
SELECT * FROM R1, R2 WHERE R1.A = R2.A AND R1.C > R2.C;
SELECT * FROM P1 JOIN R2 ON P1.A = R2.A WHERE P1.C > R2.C;
SELECT * FROM R1 JOIN R2 ON ABS(R1.A) = R2.A;
SELECT * FROM R1 JOIN R2 ON R1.A = R2.A AND R1.C = R2.C;
SELECT * FROM R1, R2 WHERE R1.A = R2.A AND R1.C = R2.C;
SELECT * FROM R1 JOIN R2 USING (A,C);
SELECT * FROM P1 JOIN R2 USING (A,C);
select * FROM P1 JOIN R1 ON P1.A = R1.A JOIN R2 ON P1.C = R2.C WHERE P1.A > 0;
