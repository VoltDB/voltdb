select * FROM R1 LEFT JOIN R2 ON R1.A = R2.C;
select * FROM R1 LEFT JOIN R2 ON R1.A = R2.C;
select * FROM R2 RIGHT JOIN R1 ON R1.A = R2.C;
select * FROM P1 LEFT JOIN R2 ON P1.A = R2.C;
select * FROM R1 LEFT JOIN R2 ON R1.A = R2.C AND R1.C = 1;
select * FROM R2 RIGHT JOIN R1 ON R1.A = R2.C AND R1.C = 1;
select * FROM R2 RIGHT JOIN P1 ON P1.A = R2.C AND P1.C = 1;
