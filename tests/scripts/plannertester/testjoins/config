select * FROM R2 LEFT JOIN R3 ON R3.A = R2.A;
select * FROM R3 RIGHT JOIN R2 ON R3.A = R2.A;
select * FROM P2 LEFT JOIN R3 ON R3.A = P2.A;
select * FROM R2 LEFT JOIN R3 ON R3.A = R2.A AND R2.C < 0;
select * FROM R3 RIGHT JOIN R2 ON R3.A = R2.A AND R2.C < 0;
select * FROM P2 LEFT JOIN R3 ON R3.A = P2.A AND P2.E < 0;
