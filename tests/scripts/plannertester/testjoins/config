select * FROM R1 LEFT JOIN R2 ON R1.A = R2.C WHERE R2.A IS NULL;
select * FROM P1 LEFT JOIN R2 ON P1.A = R2.C WHERE R2.A IS NULL;
select * FROM R1 LEFT JOIN R2 ON R1.A = R2.C WHERE R1.C = 1;
