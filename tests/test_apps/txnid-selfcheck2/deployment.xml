<deployment>
    <cluster hostcount="1" sitesperhost="4" kfactor="0" schema="ddl" />
    <commandlog enabled="true" synchronous="true" />
    <httpd enabled="true">
        <jsonapi enabled="true" />
    </httpd>
    <paths>
        <commandlog path="/tmp/testlog/" />
	<!-- <commandlogsnapshot path="/tmp/" /> -->
    </paths>
    <export enabled="true" target="file">
        <configuration>
            <property name="type">csv</property>
            <property name="nonce">MyExport</property>
        </configuration>
    </export>
</deployment>
