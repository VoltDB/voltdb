CREATE TABLE t1 (
  id       bigint not null,
  col1     tinyint,
  col2     smallint,
  col3     integer,
  col4     float,
  col5     decimal,
  col6     geography,
  col7     geography_point,
  col8     varchar(1),
  col9     varchar(10),
  col10    varchar(500),
  col11    varbinary(1000),
  col12    timestamp,
  primary key (id)
);

desc t1;

PARTITION TABLE t1 ON COLUMN id;

DESCRIBE t1;

CREATE VIEW v1 (id, records,total) AS
SELECT id, COUNT(*), SUM(col5) FROM t1 GROUP BY id;

DESCRIBE v1;

CREATE STREAM s1 PARTITION ON COLUMN id (
    id BIGINT NOT NULL,
    ts TIMESTAMP
);

DESCRIBE s1;

describe t2;

DR TABLE t1;
describe t1;
