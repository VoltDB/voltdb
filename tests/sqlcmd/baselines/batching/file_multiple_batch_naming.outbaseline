CREATE TABLE t2 (p INTEGER NOT NULL, i INTEGER ASSUMEUNIQUE, a INTEGER, s FLOAT, PRIMARY KEY(i, p));

CREATE PROCEDURE testp AS SELECT a, s FROM t ORDER BY s DESC LIMIT 1;
CREATE TABLE t3 (i INTEGER, p INTEGER, PRIMARY KEY(i));
CREATE VIEW mt (p, n) AS SELECT p, COUNT(*) FROM t GROUP BY p;
CREATE TABLE t4 (
    c1 VARCHAR(16) NOT NULL,
    c2 BIGINT DEFAULT 0 NOT NULL,
    c3 VARCHAR(36) DEFAULT '' NOT NULL
);

CREATE VIEW vt4(c1, c2, total)
    AS SELECT c1, c2, COUNT(*)
    FROM t4
    GROUP BY c1, c2;

